// Core type definitions for Unreal Engine

input int UE_VERSION;
input bool WITH_CASE_PRESERVING_NAME = false;

// Definitions for types with explicit sizes
public type int8_t = char;
public type uint8_t = unsigned char;
public type int16_t = short int;
public type uint16_t = unsigned short int;
public type int32_t = int;
public type uint32_t = unsigned int;
public type int64_t = long long int;
public type uint64_t = unsigned long long int;
